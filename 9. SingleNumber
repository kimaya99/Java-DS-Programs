class Solution {
    public int singleNumber(int[] nums) {
        Set<Integer> hashSet = new HashSet<Integer>();
       
        for(int i=0;i<nums.length;i++){
            if(!hashSet.contains(nums[i]))         
                hashSet.add(nums[i]);
            else
			    hashSet.remove(nums[i]);			
	        }
	return hashSet.iterator().next();
    }
}
